		for(int i = 0; i < table.length; i++)
			{
			for(int j = 0; j < table[0].length; j++)
				{
				if(table[i][j]) System.out.print("T ");
				else System.out.print("F ");
				}
			System.out.println();
			}
		System.out.println("Done! Here's the split: ");
		for(int i = 0; i < markers.length; i++)
			{
			if(markers[i]) System.out.print("T ");
			else System.out.print("F ");
			}
		System.out.println();
		System.out.println();
		System.out.println();
